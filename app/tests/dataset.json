      "expected_output": {
        "filters": [
          {"field": "doesnt_have_email", "operator": "=", "value": true}
        ]
      }
    },
    {
      "id": 16,
      "prompt_en": "Customers with no orders",
      "prompt_ar": "العملاء الذين ليس لديهم طلبات",
      "expected_output": {
        "filters": [
          {"field": "doesnt_have_orders", "operator": "=", "value": true}
        ]
      }
    },
    {
      "id": 17,
      "prompt_en": "Customers who have cancelled orders",
      "prompt_ar": "العملاء الذين لديهم طلبات ملغاة",
      "expected_output": {
        "filters": [
          {"field": "have_cancelled_orders", "operator": "=", "value": true}
        ]
      }
    },
    {
      "id": 18,
      "prompt_en": "Customers with rating not equal to 1",
      "prompt_ar": "العملاء الذين تقييمهم لا يساوي 1",
      "expected_output": {
        "filters": [
          {"field": "store_rating", "operator": "!=", "value": 1}
        ]
      }
    },
    {
      "id": 19,
      "prompt_en": "Male customers in Riyadh with sales between 1000 and 5000",
      "prompt_ar": "العملاء الذكور في الرياض الذين لديهم مبيعات بين 1000 و 5000",
      "expected_output": {
        "filters": [
          {"field": "gender", "operator": "=", "value": "Male"},
          {"field": "city", "operator": "=", "value": "Riyadh"},
          {"field": "total_sales", "operator": "between", "value": [1000, 5000]}
        ]
      }
    },
    {
      "id": 20,
      "prompt_en": "Customers from UAE",
      "prompt_ar": "العملاء من الإمارات",
      "expected_output": {
        "filters": [
          {"field": "country", "operator": "=", "value": "UAE"}
        ]
      }
    }
  ]
}

